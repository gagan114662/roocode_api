{
  "name": "roo-code",
  "version": "1.0.0",
  "description": "Roo Code API and Extension",
  "main": "dist/extension.js",
  "scripts": {
    "test": "jest",
    "test:api": "jest -c jest.config.api.js",
    "test:api:watch": "jest -c jest.config.api.js --watch",
    "test:api:coverage": "jest -c jest.config.api.js --coverage",
    "test:services": "jest -c jest.config.services.js",
    "test:services:watch": "jest -c jest.config.services.js --watch",
    "test:services:coverage": "jest -c jest.config.services.js --coverage",
    "test:services:unit": "jest -c jest.config.services.js --selectProjects unit",
    "test:services:integration": "INTEGRATION=true jest -c jest.config.services.js --selectProjects integration --runInBand",
    "test:services:all": "INTEGRATION=true jest -c jest.config.services.js --runInBand",
    "start:api": "ts-node -r dotenv/config src/api/start.ts",
    "build:api": "tsc -p src/api/tsconfig.json",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "worker": "ts-node -r dotenv/config src/api/workers/index.ts",
    "postinstall": "npm run build:api"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/roo-code.git"
  },
  "keywords": [
    "roo",
    "code",
    "api",
    "vscode",
    "extension"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@google-cloud/vertexai": "^1.9.3",
    "@modelcontextprotocol/sdk": "^1.10.1",
    "@types/glob": "^8.1.0",
    "@types/vscode": "^1.99.1",
    "axios": "^1.8.4",
    "bullmq": "^5.1.1",
    "clone-deep": "^4.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "glob": "^11.0.1",
    "helmet": "^7.2.0",
    "i18next": "^25.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "openai": "^4.95.1",
    "prom-client": "^15.1.0",
    "simple-git": "^3.27.0",
    "swagger-ui-express": "^5.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@anthropic-ai/vertex-sdk": "^0.7.0",
    "@aws-sdk/client-bedrock-runtime": "^3.787.0",
    "@aws-sdk/credential-providers": "^3.787.0",
    "@google/generative-ai": "^0.24.0",
    "@jest/globals": "^29.7.0",
    "@mistralai/mistralai": "^1.6.0",
    "@types/cheerio": "^0.22.35",
    "@types/chokidar": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/diff": "^7.0.2",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/helmet": "^4.0.0",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.30",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/turndown": "^5.0.5",
    "@typescript-eslint/eslint-plugin": "8.30.1",
    "@typescript-eslint/parser": "8.30.1",
    "delay": "^6.0.0",
    "diff": "^7.0.0",
    "eslint": "^8.57.0",
    "fast-xml-parser": "^5.2.0",
    "fastest-levenshtein": "^1.0.16",
    "fzf": "^0.5.2",
    "globby": "^14.1.0",
    "isbinaryfile": "^5.0.4",
    "jest": "^29.7.0",
    "jest-simple-dot-reporter": "^1.0.5",
    "js-tiktoken": "^1.0.19",
    "mammoth": "^1.9.0",
    "os-name": "^6.0.0",
    "p-wait-for": "^5.0.2",
    "pdf-parse": "^1.1.1",
    "posthog-node": "^4.14.0",
    "prettier": "^3.2.5",
    "reconnecting-eventsource": "^1.6.4",
    "strip-ansi": "^7.1.0",
    "strip-bom": "^5.0.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">=18"
  }
}
